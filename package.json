{
  "name": "ts-clasp-starter-pack",
  "version": "1.0.0",
  "description": "Google Apps Script project starter pack with TypeScript support",
  "main": "build/index.js",
  "scripts": {
    "build": "rollup --config",
    "build-dev": "rollup --config --watch",
    "lint": "gts lint",
    "fix": "gts fix",
    "deploy": "npm run build & clasp push",
    "dev": "clasp push --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@google/clasp": "^2.3.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/google-apps-script": "^1.0.31",
    "@types/node": "^12.20.10",
    "gts": "^3.1.0",
    "rollup": "^2.45.2",
    "typescript": "^4.2.4"
  },
  "keywords": [
    "clasp",
    "google-apps-script",
    "gas",
    "typescript",
    "rollup-js",
    "prettier",
    "eslint",
    "javascript"
